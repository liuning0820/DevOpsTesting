{
  "name": "testing-poc-nodejs",
  "version": "1.0.0",
  "description": "Node.js testing poc",
  "main": "app.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "start": "node app.js",
    "test": "mocha --recursive --exit",
    "coverage": "nyc --reporter=html mocha --recursive --exit",
    "coverage-check": "nyc --reporter=html check-coverage --lines 80 --functions 80 --branches 80 mocha --recursive --exit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.dxc.com/thalpin2/testing-poc-nodejs.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1"
  }
}
